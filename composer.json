{
    "name": "gliterd/laravel-smartystreets",
    "type": "library",
    "description": "Smarty Streets Service Provider for Laravel 5",
    "keywords": ["smartystreets", "location", "api", "laravel"],
    "homepage": "https://github.com/mhetreramesh/flysystem-backblaze",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramesh Mhetre",
            "email": "ramesh@about.me",
            "homepage": "https://about.me/rameshmhetre",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "gliterd/smartystreets": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Gliterd\\SmartyStreets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gliterd\\SmartyStreets\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Gliterd\\SmartyStreets\\SmartyStreetsServiceProvider"
            ]
        }
    }
}
